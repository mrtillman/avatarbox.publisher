{
  "name": "avatarbox.publisher",
  "version": "1.0.0",
  "description": "hourly update routine for avatarbox.io",
  "main": "index.js",
  "dependencies": {
    "avatarbox.sdk": "file:../sdk"
  },
  "scripts": {
    "zip": "rm -f avbx-publisher.zip && zip -r avbx-publisher.zip .",
    "deploy": "aws lambda update-function-code --function-name avbx-publisher --zip-file fileb://avbx-publisher.zip"
  },
